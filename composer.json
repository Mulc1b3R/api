{
    "name": "api-platform/api-platform",
    "type": "project",
    "license": "proprietary",
    "description": "Project description",
    "require": {
        "php": "^7.1.3",
        "api-platform/api-pack": "2.1.x-dev",
        "api-platform/core": "2.1.x-dev",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BJDRR317TRYMRB794H7RVT46",
            "allow-contrib": false
        }
    }
}
